{
  "name": "node-swagger",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "pm2 install pm2-logrotate; pm2 restart $npm_package_config_pm2config --only api --env prod",
    "stop": "pm2 stop $npm_package_config_pm2config --only api",
    "restart": "pm2 restart $npm_package_config_pm2config --only api",
    "start-dev": "nx serve api",
    "build": "rm -rf dist/ && nx build",
    "test": "nx run-many --all --target=test --coverage --colors --reporters=jest-junit",
    "test-dev": "nx test --coverage --colors",
    "clear_jest": "jest --clearCache",
    "pm2-update": "pm2 update"
  },
  "jest-junit": {
    "suiteName": "marin-publisher-pg-service",
    "outputDirectory": "./target",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "config": {
    "pm2config": "pm2.json"
  },
  "private": true,
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "10.3.8",
    "@nestjs/core": "10.3.8",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "10.3.8",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/terminus": "^10.0.0",
    "dates-between": "^5.0.0",
    "jest-junit": "^15.0.0",
    "libcore": "^0.3.9",
    "moment": "^2.29.4",
    "mongoose": "^6.2.6",
    "mysql2": "^3.9.7",
    "nest-winston": "^1.6.2",
    "nestjs-pino": "^3.3.0",
    "nodemailer": "^6.9.0",
    "nx": "16.10.0",
    "papaparse": "^5.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pattern-replace": "^0.1.2",
    "pino-elasticsearch": "^6.2.0",
    "pino-http": "^6.6.0",
    "pm2": "^5.2.0",
    "properties": "^1.2.1",
    "properties-reader": "^2.2.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sendgrid": "^5.2.1",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.6.2",
    "unique-names-generator": "^4.7.1",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-elasticsearch": "^0.16.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.8",
    "@nrwl/cli": "15.0.0",
    "@nrwl/tao": "16.10.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/nest": "16.10.0",
    "@nx/node": "16.10.0",
    "@nx/webpack": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@types/jest": "29.4.4",
    "@types/node": "20.12.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "~28.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "29.1.2",
    "typescript": "^5.1.6"
  }
}
