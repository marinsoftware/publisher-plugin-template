{
  "name": "node-swagger",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "pm2 install pm2-logrotate; pm2 restart $npm_package_config_pm2config --only api --env prod",
    "stop": "pm2 stop $npm_package_config_pm2config --only api",
    "restart": "pm2 restart $npm_package_config_pm2config --only api",
    "start-dev": "nx serve api | pino-pretty",
    "build": "rm -rf dist/ && nx build",
    "test": "nx test --coverage --colors --reporters=jest-junit",
    "clear_jest": "jest --clearCache",
    "pm2-update": "pm2 update"
  },
  "config": {
    "pm2config": "pm2.json"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/terminus": "^8.0.4",
    "mongoose": "^6.2.6",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.6.2",
    "nestjs-pino": "^2.5.0",
    "nx": "^15.0.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pattern-replace": "^0.1.2",
    "pino-elasticsearch": "^6.2.0",
    "pino-http": "^6.6.0",
    "pm2": "^5.2.0",
    "properties": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3",
    "supertest": "^6.2.2",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.0.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-elasticsearch": "^0.16.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "13.8.8",
    "@nrwl/eslint-plugin-nx": "13.8.8",
    "@nrwl/jest": "13.8.8",
    "@nrwl/linter": "13.8.8",
    "@nrwl/nest": "13.8.8",
    "@nrwl/node": "13.8.8",
    "@nrwl/tao": "13.8.8",
    "@nrwl/workspace": "13.8.8",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "gelf-pro": "^1.3.9",
    "jest": "27.2.3",
    "jest-junit": "^14.0.1",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.5.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2",
    "underscore": "^1.13.6"
  },
  "jest-junit": {
    "suiteName": "node-swagger-nestjs",
    "outputDirectory": "./target",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
