<!doctype html>

<html lang="en">
<head>
    <meta charset="utf-8"/>
    <title>{{#apiInfo}}{{title}}{{/apiInfo}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="Content-Language" content="en" />

    <link rel="stylesheet" href="../css/apache-maven-fluido-1.3.0.min.css" />
    <link rel="stylesheet" href="../css/site.css" />
    <link rel="stylesheet" href="../css/print.css" media="print" />
    <script type="text/javascript" src="../js/apache-maven-fluido-1.3.0.min.js"></script>

    <style>
        .activeHeader {
            color: #fff;
            text-shadow: 0 -1px 0 rgba(0,0,0,0.2);
            background-color: #08c;
            border-radius: 5px;
            padding: 4px 10px;
            margin: 10px;
        }
    </style>
</head>
<body class="topBarDisabled">
<div id="banner" class="activeHeader">

    <h2>BasePath: {{basePath}}</h2>

</div>
<div class="container-fluid">

    <div id="breadcrumbs">
        <ul class="breadcrumb">
            <li id="projectVersion">{{apiVersion}}</li>
        </ul>
    </div>

    <h4>{{description}}</h4>

    <div id="api_container">
        <h3>Resources</h3>
        {{#apiDocuments}}
            <h4>{{resourcePath}}</h4>

            <div class='content'>
                {{{description}}}


                <div id="accordion{{index}}api">
                    {{#apis}}
                        {{#operations}}
                            <h5><span style="font-weight: bold;">{{httpMethod}} </span><span>{{{path}}}</span>

                                <div id="notes">{{{summary}}}</div>
                            </h5>
                            <div>
                                <p>{{notes}}</p>

                                <div id="tabs{{index}}{{apiIndex}}{{opIndex}}">

                                    <ul>
                                        <li><a href="#tabs{{index}}{{apiIndex}}{{opIndex}}-1">Request Parameters</a>
                                        </li>
                                        <li><a href="#tabs{{index}}{{apiIndex}}{{opIndex}}-2">Response Body</a></li>
                                        <li><a href="#tabs{{index}}{{apiIndex}}{{opIndex}}-3">Response Status Codes</a>
                                        </li>
                                        <li><a href="#tabs{{index}}{{apiIndex}}{{opIndex}}-4">Examples</a>
                                        </li>
                                    </ul>
                                    <div id="tabs{{index}}{{apiIndex}}{{opIndex}}-1">
                                        <ul>
                                            {{#parameters}}
                                                <li>
                                                    <h7>{{paramType}}</h7>
                                                    <table>
                                                        <tr>
                                                            <th>Name</th>
                                                            <th>Required</th>
                                                            <th>Data Type</th>
                                                            <th>Description</th>
                                                        </tr>
                                                        {{#paras}}
                                                            <tr>
                                                                <th>{{name}}</th>
                                                                <td>{{#required}}required{{/required}}
                                                                    {{^required}}optional{{/required}}</td>
                                                                <td>{{type}}</td>
                                                                <td>{{description}}</td>
                                                            </tr>
                                                        {{/paras}}
                                                    </table>
                                                </li>
                                            {{/parameters}}
                                        </ul>
                                    </div>
                                    <div id="tabs{{index}}{{apiIndex}}{{opIndex}}-2">
                                        <p>{{#responseClass}}<a
                                                href="#{{{classLinkName}}}">{{className}}</a>{{/responseClass}}
                                            {{^responseClass}}No Content{{/responseClass}}
                                        </p>
                                    </div>
                                    <div id="tabs{{index}}{{apiIndex}}{{opIndex}}-3">
                                        <table>
                                            <tr>
                                                <th>Status Code</th>
                                                <th>Reason</th>
                                            </tr>
                                            {{#errorResponses}}
                                                <tr>
                                                    <td>{{code}}</td>
                                                    <td>{{{message}}}</td>
                                                </tr>
                                            {{/errorResponses}}
                                        </table>
                                    </div>
                                    <div id="tabs{{index}}{{apiIndex}}{{opIndex}}-4">
                                        <p>{{#samples}}
                                            Sample Request

                                            <pre>
                                            {{{sampleRequest}}}
                                            </pre>

                                            - Sample Response

                                            <pre>
                                            {{{sampleResponse}}}
                                            </pre>
                                           {{/samples}}
                                           {{^samples}}No Samples{{/samples}}
                                        </p>
                                    </div>
                                </div>
                            </div>
                        {{/operations}}
                    {{/apis}}
                </div>
            </div>
        {{/apiDocuments}}
    </div>
    <h3>Data Types</h3>
    {{#dataTypes}}
        <h4><a name="{{name}}">{{name}}</a></h4>

        <div>
            <table>
                <th>Name</th>
                <th>Type</th>
                <th>Required</th>
                <th>Access</th>
                <th>Schema</th>
                <th>Notes</th>
                {{#items}}
                    <tr>
                        <th>{{name}}</th>
                        <td>{{#linkType}}<a href="#{{{linkType}}}">{{type}}</a>{{/linkType}}{{^linkType}}{{type}}{{/linkType}}</td>
                        <td>{{required}}</td>
                        <td>{{access}}</td>
                        <td>{{{description}}}</td>
                        <td>{{{notes}}}</td>
                    </tr>
                {{/items}}
            </table>
        </div>
    {{/dataTypes}}
        <h5>Terms of Service: <a href="{{termsOfServiceUrl}}">{{termsOfServiceUrl}}</a></h5>

</div>
</body>
</html>
